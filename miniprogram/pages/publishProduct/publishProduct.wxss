.page{
  background-color: #f1f2f4;
  padding-top: 20rpx;
  min-height: 100vh;
}
swiper {
  height: 620rpx;
}
.swiper-item{
  position: relative;
}
.swiper_section{
  padding: 0;
  height: 620rpx;
}
.add_img_wrap{
  width: 100%;
  height: 100%;
  background-color: #f7f8fa;
}
.add_img_icon{
  font-size: 140rpx;
  font-weight: 700;
  color: #888;
}
.image_item{
  height: 620rpx;
  width: 100%;
}

.del_img_wrap{
  position: absolute;
  right: 0rpx;
  top: 0rpx;
  width: 52rpx;
  height: 52rpx;
  background-color: #000;
  border-radius: 50%;
}
.del_img_icon{
  color: #fff;
  font-size: 28rpx;
  font-weight: 700;
}

.btn_section{
  padding: 18rpx 30rpx 40rpx;
}