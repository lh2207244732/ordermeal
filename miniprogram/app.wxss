.primary_color {
  color: #19b5fe;
}
.primary_background_color {
  background-color: #19b5fe;
}
view {
  box-sizing: border-box;
}
.flex_center{
  display: flex;
  justify-content: center;
  align-items: center;
}
.ellipsis{
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.ellipsis2{
  display: -webkit-box;    
  -webkit-box-orient: vertical;    
  -webkit-line-clamp: 2;    
  overflow: hidden;
}