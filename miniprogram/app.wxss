.primary_color {
  color: #19b5fe;
}
.primary_background_color {
  background-color: #19b5fe;
}
view {
  box-sizing: border-box;
}
.flex_center{
  display: flex;
  justify-content: center;
  align-items: center;
}
.ellipsis{
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.ellipsis2{
  display: -webkit-box;    
  -webkit-box-orient: vertical;    
  -webkit-line-clamp: 2;    
  overflow: hidden;
}

.white_card{
  background-color: #fff;
  box-shadow: rgb(149 157 165 / 20%) 0rpx 1rpx 9rpx;
  border-radius: 20rpx;
  padding: 24rpx;
  margin: 30rpx;
  overflow: hidden;
}